<h1>Mensajes</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>E-mail</th>
      <th>Mensaje</th>
    </tr>
  </thead>
  <tbody>
  {{#each model as |contact|}}
    <tr>
      <th>{{contact.id}}</th>
      <td>{{contact.email}}</td>
      <td>{{contact.message}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>